<h1>Users</h1>

<table>
  <tr>
    <th>Username</th>
    <th>Email</th>
    <th>Gender</th>
    <th>Country</th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.username %></td>
    <td><%= user.email %></td>
    <td><%= user.gender %></td>
    <td><%= user.country %></td>
  </tr>
<% end %>
</table>
